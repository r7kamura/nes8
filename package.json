  "license": "MIT",
  "bugs": {
    "url": "https://github.com/r7kamura/nes8/issues"
  },
  "homepage": "https://github.com/r7kamura/nes8#readme",
  "devDependencies": {
    "@types/node": "10.12.12",
    "prettier": "1.15.3",
    "ts-loader": "5.3.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.2.1",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2"
  }
}
