{
  "name": "nes8",
  "version": "0.1.0",
  "description": "A NES emulator written in TypeScript.",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack",
    "format": "prettier --write package.json src/**/*.ts webpack.config.js",
    "lint": "tslint src/**/*.ts",
    "test": "npm run lint && npm run build",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r7kamura/nes8.git"
  },
  "keywords": [
    "nes"
  ],
